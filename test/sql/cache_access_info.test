metadata	0	0	NULL	NULL	NULL
data	0	0	NULL	NULL	NULL
file handle	0	0	NULL	NULL	NULL
glob	0	0	NULL	NULL	NULL

# Start to record profile.
statement ok
SET cache_httpfs_profile_type='temp';

# Test uncached query.
statement ok
SELECT cache_httpfs_clear_cache();

query IIIII
