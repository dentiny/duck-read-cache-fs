statement ok
SET cache_httpfs_disk_cache_reader_enable_memory_cache=true;

statement ok
SET cache_httpfs_cache_directories_config='/tmp/duckdb_cache_httpfs_cache';

statement ok
SELECT cache_httpfs_clear_cache();

